{
  "manifest_version": 2,
  "name": "Youtube_small_window",
  "version": "2.1",
  "permissions": [ "tabs", "storage", "<all_urls>"],
  "content_scripts": [
    {
      "matches": [ "*://*.youtube.com/*" ],
      "js": [ "content_scripts/content_script.js" ] //,
      //"css": [ "content_scripts/traps.css" ]
    }
  ],
  "externally_connectable": {
    "matches": [ "*://*.youtube.com/*" ]
  },
  "browser_action": {
    "default_icon": {
      "64": "images/play_96.png"
    },
    "default_title": "Youtube small window",
    "default_popup": "popup/popup.html"
  }
}